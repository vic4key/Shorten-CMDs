@ECHO OFF

ECHO dns-list     - Display DNS Server list
ECHO dns-list-if  - Display Device's Network Interface list
ECHO dns-select-* - Select a DNS server
ECHO    dns-select-auto - Select DHCP Automatically
ECHO    dns-select-rt   - Select Default Router's DNS Server
ECHO    dns-select-gg   - Select Google's DNS Server
ECHO    dns-select-cf   - Select Cloudflare's DNS Server
ECHO    dns-select-od   - Select OpenDNS's DNS Server