@ECHO OFF

ECHO Git shorten commands :

ECHO    git-add          - Add files to indexing in local repository
ECHO    git-changes      - Equal to `git-status`
ECHO    git-checkout     - Equal to `git-clone`
ECHO    git-clone        - Clone a remote repository to local repository
ECHO    git-commit       - Commit ^& Push changes to remote repository
ECHO    git-commit-force - Force commit ^& push changes to remote repository
ECHO    git-delete-head  - Delete the last commit HEAD in remote repository
ECHO    git-edit-head    - Edit the last commit HEAD in remote repository
ECHO    git-diff         - Diff changes repository
ECHO    git-log          - View log in remote repository
ECHO    git-pull         - Pull changes from remote repository
ECHO    git-remove       - Remove files from indexing in local repository
ECHO    git-revert       - Revert changes in local repository
ECHO    git-status       - View changes in local repository
ECHO    git-update       - Equal to `git-pull`
ECHO    git-sub-add      - Add a sub-module to repository
ECHO    git-sub-clone    - Clone sub-modules of repository
ECHO    git-sub-remove   - Remove a sub-module in repository
ECHO    git-sub-update   - Clone ^& Update sub-modules in repository
